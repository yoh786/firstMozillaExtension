{

  "manifest_version": 2,
  "name": "Borderify",
  "version": "1.0",
  "description": "adds red border to all domains matching mozilla.org",
  "icons": {
    "48": "icons/border-48.png"
  },
  "content_scripts": [
    {
      "matches": ["*://*.mozilla.org/*"],
      "js": ["borderify.js"]
    }
  ]
  
}
